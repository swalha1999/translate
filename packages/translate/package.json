{
  "name": "@swalha1999/translate",
  "version": "0.3.0",
  "description": "AI-powered translation for user-generated content with intelligent caching",
  "author": "swalha1999",
  "repository": {
    "type": "git",
    "url": "https://github.com/swalha1999/translate.git",
    "directory": "packages/translate"
  },
  "homepage": "https://github.com/swalha1999/translate#readme",
  "bugs": {
    "url": "https://github.com/swalha1999/translate/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js",
      "types": "./dist/adapters/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "ai": "^6.0.3",
    "@ai-sdk/openai": "^3.0.1",
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/mistral": "^3.0.1",
    "@ai-sdk/groq": "^3.0.1"
  },
  "devDependencies": {
    "tsdown": "^0.9.5",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "translation",
    "ai",
    "openai",
    "anthropic",
    "gemini",
    "mistral",
    "groq",
    "cache",
    "ugc",
    "i18n"
  ],
  "license": "MIT"
}
